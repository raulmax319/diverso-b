{
  "name": "diverso",
  "version": "1.0.0",
  "description": "Backend diverso",
  "main": "index.js",
  "author": "Raul Max",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:clean && node dist",
    "build": "node ./node_modules/typescript/bin/tsc -p tsconfig.json",
    "build:paths": "npx tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:clean": "npm run clean && npm run build && npm run build:paths",
    "clean": "rm -rf dist",
    "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./src",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "lint": "eslint src/**"
  },
  "dependencies": {
    "@prisma/client": "5.8.1",
    "axios": "1.6.5",
    "bcrypt": "5.1.1",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "jsonwebtoken": "9.0.2",
    "prisma": "5.8.1",
    "resdk": "github:raulmax319/reSDK",
    "tslog": "4.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/http-errors": "2.0.4",
    "@types/jsonwebtoken": "9.0.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.10.6",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "dotenv": "16.3.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.2",
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
